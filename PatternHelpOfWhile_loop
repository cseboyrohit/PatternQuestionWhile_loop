  /*    // Pattern Question:-10
//        Enter Digit:- 4
//        A B C D
//        E F G H
//        I J K L
//        M N O P

        char currentChar = 'A'; // Start with character 'A'
        int row = 1; // Initialize row counter

        while (row <= n) { // Loop for each row
            int col = 1; // Initialize column counter
            while (col <= n) { // Loop for each column
                System.out.print(currentChar + " "); // Print the current character
                currentChar++; // Move to the next character
                col++; // Increment column counter
            }
            System.out.println(); // Move to the next line after printing each row
            row++; // Increment row counter
        }

     */

