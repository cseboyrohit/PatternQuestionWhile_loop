       /* public class Main {
            public static void main(String[] args) {
                int rows = 4; // Number of rows

                // Outer loop for rows
                int i = 1;
                while (i <= rows) {
                    int spaces = 1;
                    // Inner loop for spaces
                    while (spaces <= rows - i) {
                        System.out.print(" ");
                        spaces++;
                    }

                    int j = 1;
                    // Inner loop for numbers increasing from 1 to i
                    while (j <= i) {
                        System.out.print(j);
                        j++;
                    }

                    j = i - 1;
                    // Inner loop for numbers decreasing from i-1 to 1
                    while (j >= 1) {
                        System.out.print(j);
                        j--;
                    }

                    System.out.println(); // Move to the next line
                    i++;
                }
            }
        }

        */



