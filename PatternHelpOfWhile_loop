     /*   int i = 1;
        while (i <= n) {
            int j = 1;
            // Inner loop for spaces
            while (j <= i) {
                System.out.print(" ");
                j++;
            }

            int k = 1;
            // Inner loop for numbers
            while (k <= n - i + 1) { // similar to above code but row print
                System.out.print(i);
                k++;
            }

            System.out.println(); // Move to the next line
            i++;
        }
