                System.out.print(j+" ");
                j--;
            }
            i++;
            System.out.println();
        }


        // Now Using Formula
        int i=1;
        while(i<=n){
            int j=0;
            while(j<n){
                int sum =n-j; // Using formula Created by programmer
                System.out.print(sum);
                j++;
            }
            i++;
            System.out.println();
        }




